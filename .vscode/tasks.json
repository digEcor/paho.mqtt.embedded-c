{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "showOutput": "always",
    "suppressTaskName": true,
    "options": {
        "cwd": "${workspaceRoot}"
    },
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "./travis-build.sh",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}