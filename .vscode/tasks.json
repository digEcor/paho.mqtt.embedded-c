{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "presentation":{
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared"
    },
    "suppressTaskName": true,
    "options": {
        "cwd": "${workspaceRoot}"
    },
    "tasks": [
        {
            "taskName": "build",
            "type": "shell",
            "command": "./travis-build.sh",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}